<?xml version='1.0' encoding='utf-8'?>
<widget id="us.wearecurio.app" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Keyboard">
        <param name="android-package" value="com.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PushPlugin">
        <param name="android-package" value="com.plugin.gcm.PushPlugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="StatusBar">
        <param name="android-package" onload="true" value="org.apache.cordova.statusbar.StatusBar" />
    </feature>
    <name>Curious</name>
    <description>Track Data, chart experience, find meaning.</description>
    <author email="support@wearecurio.us" href="http://dev.wearecurio.us">Curious Inc</author>
    <icon src="icon.png" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <access origin="*" />
    <content src="index.html" />
</widget>
